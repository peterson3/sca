{
  "MongoConnection": {
    "ConnectionString": "mongodb://root:example@localhost",
    "Database": "BarragensDB",
    "IsSSL": false
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "spring": {
    "application": {
      "name": "BarragemService"
    }
  },
  "eureka": {
    "client": {
      "serviceUrl": "http://localhost:8761/eureka/",
      "shouldFetchRegistry": "false",
      "shouldRegisterWithEureka": true,
      "validateCertificates": false
    },
    "instance": {
      "appName": "BarragemService",
      "hostName": "localhost",
      "port": "5000"
    }
  },
  "AllowedHosts": "*"
}
