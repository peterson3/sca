{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "spring": {
    "application": { "name": "Ocelot-Gateway" },
    "cloud": {
      "config": {
        "uri": "http://sca-api.brazilsouth.azurecontainer.io",
        "validateCertificates": false
      }
    }
  },
  "eureka": {
    "client": {
      "serviceUrl": "http://sca-eurekacontainer.brazilsouth.azurecontainer.io:8761/eureka/",
      "shouldRegisterWithEureka": false,
      "validateCertificates": false
    }
  },
  "AllowedHosts": "*"
}
